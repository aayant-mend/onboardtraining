{
  "scanSettings": {
    "baseBranches": []
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "displayMode": "diff",
    "useMendCheckNames": true
  },
  "issueSettings": {
    "minSeverityLevel": "LOW",
    "issueType": "DEPENDENCY"
  },
  "packageRules": [
            {
                "matchUpdateTypes": [
                    "minor",
                    "patch",
                    "pin",
                    "digest"
                ],
                "automerge": true
            },
            {
                "matchDepTypes": [
                    "devDependencies"
                ],
                "automerge": true
            }
        ]
}


